<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.BooksController">
   <children>
      <TextField layoutX="72.0" layoutY="73.0" promptText="Enter Text to Search" />
      <Button layoutX="243.0" layoutY="73.0" mnemonicParsing="false" text="Search" />
      <TableView layoutX="72.0" layoutY="114.0" prefHeight="368.0" prefWidth="588.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button layoutX="386.0" layoutY="526.0" mnemonicParsing="false" text="Remove Book" />
      <Button fx:id="btnNew" layoutX="582.0" layoutY="526.0" mnemonicParsing="false" onAction="#ButtonNew" text="New Book" />
      <Button fx:id="btnBack" layoutX="72.0" layoutY="526.0" mnemonicParsing="false" onAction="#ButtonBack" text="Back" />
      <Button fx:id="btnAdd" layoutX="498.0" layoutY="526.0" mnemonicParsing="false" onAction="#ButtonAdd" text="Add Book" />
   </children>
</AnchorPane>
